  "grammar": {
    "sentencePatterns": {
      "title": "基本句型 Basic Sentence Patterns",
      "patterns": [
        {
          "name": "SV 主谓结构",
          "formula": "S (subject) + V (verb)",
          "description": "主语做什么，什么动作发生在主语身上",
          "examples": [
            {"s": "The bus", "v": "stops."},
            {"s": "The school gate", "v": "opens."},
            {"s": "Yiming", "v": "smiles."},
            {"s": "It", "v": "rains."}
          ]
        },
        {
          "name": "SVC 主动补结构",
          "formula": "S (subject) + V (linking verb) + C (complement)",
          "description": "主语是什么，怎么样",
          "examples": [
            {"s": "My favourite colour", "v": "is", "c": "green."},
            {"s": "The dress", "v": "looks", "c": "nice!"},
            {"s": "We", "v": "are", "c": "middle school students."},
            {"s": "Harry", "v": "is", "c": "my friend."},
            {"s": "The water", "v": "feels", "c": "warm."},
            {"s": "I", "v": "am", "c": "sorry."}
          ]
        },
        {
          "name": "SVO 主谓宾结构",
          "formula": "S (subject) + V (verb) + O (object)",
          "description": "主语对宾语做了什么动作",
          "examples": [
            {"s": "Yiming", "v": "opens", "o": "the schoolbag."},
            {"s": "Bailing", "v": "asks", "o": "a question."},
            {"s": "The students", "v": "are introducing", "o": "themselves."},
            {"s": "They", "v": "wear", "o": "red scarves."}
          ]
        }
      ]
    },
    "presentSimple": {
      "title": "一般现在时 Present Simple",
      "usage": ["习惯、常规、预定事件", "普遍真理和事实"],
      "examples": [
        "We walk to school every day.",
        "The school bus arrives at 8 o'clock.",
        "The sun rises in the east and sets in the west.",
        "The school year starts in September and ends in June."
      ],
      "forms": {
        "affirmative": [
          {"subject": "I/You/We/They", "verb": "like", "rest": "the school garden."},
          {"subject": "He/She/It", "verb": "likes", "rest": "the school garden."}
        ],
        "negative": [
          {"subject": "I/You/We/They", "verb": "don't (do not) like", "rest": "loud music."},
          {"subject": "He/She/It", "verb": "doesn't (does not) like", "rest": "loud music."}
        ],
        "yesNoQuestion": [
          {"auxiliary": "Do", "subject": "I/you/we/they", "verb": "run fast?", "answer": "Yes, I/you/we/they do. / No, I/you/we/they don't."},
          {"auxiliary": "Does", "subject": "he/she/it", "verb": "run fast?", "answer": "Yes, he/she/it does. / No, he/she/it doesn't."}
        ],
        "whQuestion": [
          {"questionWord": "What time", "auxiliary": "does", "subject": "your school", "verb": "start?"},
          {"questionWord": "How", "auxiliary": "do", "subject": "you", "verb": "go to school?"},
          {"questionWord": "Where", "auxiliary": "do", "subject": "they", "verb": "have class?"}
        ]
      }
    },
    "presentContinuous": {
      "title": "现在进行时 Present Continuous",
      "usage": ["说话时正在进行的动作", "当前一段时间内进行的活动（但不一定在说话时进行）"],
      "examples": [
        "Look! My brothers are playing football.",
        "My sister is studying for her exams these days.",
        "My brother is doing an experiment these days."
      ],
      "forms": {
        "affirmative": [
          {"subject": "I", "be": "am/am not", "verb": "playing", "rest": "in the yard."},
          {"subject": "You/We/They", "be": "are/are not/aren't", "verb": "playing", "rest": "in the yard."},
          {"subject": "He/She/It", "be": "is/is not/isn't", "verb": "playing", "rest": "in the yard."}
        ]
      },
      "ingRules": [
        {"rule": "大多数动词", "change": "+ ing", "example": "eat → eating"},
        {"rule": "以不发音的e结尾的动词", "change": "去e，再加ing", "example": "make → making, ride → riding"},
        {"rule": "部分以\"一个元音+一个辅音\"结尾的动词", "change": "双写结尾辅音字母，再加ing", "example": "sit → sitting, run → running"}
      ]
    },
